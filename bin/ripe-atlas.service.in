[Unit]
Description=Atlas Probe
After=network-online.target syslog.target

[Service]
User=@ripe_atlas_user@
Group=@ripe_atlas_user@
Environment=HOME=@localstatedir@
WorkingDirectory=@localstatedir@
ExecStart=@rpm_base_dir@/bin/ATLAS
ExecStop=/bin/sudo -u atlasmsm /bin/killall telnetd perd eperd eooqd
Restart=always
TimeoutStopSec=60

[Install]
WantedBy=multi-user.target
